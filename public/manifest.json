{
  "manifest_version": 2,
  "name": "InfoCard",
  "version": "0.0.2",
  "description": "Infocard 是一款chrome 扩展，在你每次打开新标签页时随机展示知识卡片, 利用零碎的时间学习知识。",
  "icons": {
    "16": "favicon.ico"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'sha256-5As4+3YpY62+l38PsxCEkjB1R4YtyktBtRScTJ3fyLU='; object-src 'self'",
  "permissions": [
    "storage"
  ]
}